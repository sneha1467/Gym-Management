  "client_script": "// THIS WILL RUN AFTER THE WEB FORM IS FULLY LOADED\nfrappe.web_form.after_load = function (web_form) {\n    // 1: Hide Frappe's default buttons\n    const actions = document.querySelector('.web-form-actions');\n    if (actions) {\n        actions.style.display = 'none';\n    }\n\n    // 2: Create your custom button\n    const btn = document.createElement('button');\n    btn.type = 'button';\n    btn.className = 'btn btn-primary';\n    btn.innerText = 'Login Sucessfully';\n    btn.style.margin = '10px 0';\n\n    // 3: Add button event listener\n    btn.addEventListener('click', async () => {\n        // Read form values\n        const full = frappe.web_form.get_value('full_name');\n        const email = frappe.web_form.get_value('email');\n        const pwd = frappe.web_form.get_value('password');\n\n        // Basic validation\n        if (!full || !email || !pwd) {\n            frappe.msgprint('All fields are required.');\n            return;\n        }\n\n        try {\n            await frappe.web_form.submit();\n            frappe.msgprint('Registered successfully!');\n            // Optional redirect:\n            // window.location.href = '/thank-you';\n        } catch (err) {\n            console.error(err);\n            frappe.msgprint('Failed to register. Please try again.');\n        }\n    });\n\n    // 4: Append custom button to the footer\n    const footer = document.querySelector('.web-form-footer');\n    if (footer) {\n        footer.appendChild(btn);\n    }\n};\n",
